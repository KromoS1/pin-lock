{
	"expo": {
		"name": "Secret Lock",
		"slug": "secret-lock",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "secl",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#2f9b2d"
		},
		"android": {
			"versionCode": 1,
			"package": "com.techocean.secret_lock_apk",
			"adaptiveIcon": {
				"backgroundColor": "#2f9b2d",
				"foregroundImage": "./assets/images/icon_logo.png"
			},
			"permissions": [
				"USE_BIOMETRIC",
				"android.permission.USE_BIOMETRIC",
				"android.permission.USE_FINGERPRINT"
			]
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			"expo-localization",
			"expo-local-authentication"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "8061b163-0152-4ca6-be4f-0abaa1559d4b"
			}
		}
	}
}
